{
  "manifest_version": 2,

  "name": "GitHub PR Helper",
  "description": "This adds sorely needed features to GitHub's PR pages.",
  "version": "1.0",

  "permissions": [  ],
  "content_scripts": [{
    "matches": ["https://github.com/*/pull*"],
    "css": ["styles.css"],
    "js": ["lib/angular.js", "labels.js", "listGroupItems.js", "discussionSidebar.js", "extension.js"]
  }]
}